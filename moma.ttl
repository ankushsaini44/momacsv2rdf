@prefix m:    <http://rdfdata.org/models/moma/> . 
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> . 
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> . 
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> . 

m:department      rdf:type rdf:Property .

m:heightCm        rdf:type rdf:Property ;
                  rdfs:range xsd:float .

m:widthCm         rdf:type rdf:Property ;
                  rdfs:range xsd:float .

m:depthCm         rdf:type rdf:Property ;
                  rdfs:range xsd:float .

m:dimensionsNote  rdf:type rdf:Property .

m:classification  rdf:type rdf:Property .

m:curatorApproved rdf:type rdf:Property ;
                  rdfs:range xsd:float .

m:dateAcquired    rdf:type rdf:Property ;
                  rdfs:range xsd:date .

m:workStartDate   rdf:type rdf:Property ;
                  rdfs:range xsd:date .

m:workFinishDate  rdf:type rdf:Property ;
                  rdfs:range xsd:date .
